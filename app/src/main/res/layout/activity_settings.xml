<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingsActivity">

  <Button
      android:id="@+id/button4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Update"
      tools:layout_editor_absoluteX="131dp"
      tools:layout_editor_absoluteY="591dp" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Sound Effects"
      app:layout_constraintBottom_toTopOf="@+id/textView9"
      app:layout_constraintEnd_toStartOf="@+id/switch1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView14" />

  <Switch
      android:id="@+id/switch1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Switch"
      app:layout_constraintBottom_toTopOf="@+id/switch2"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.727"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView14"
      app:layout_constraintVertical_bias="0.571" />

  <Switch
      android:id="@+id/switch2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="128dp"
      android:text="Switch"
      app:layout_constraintBottom_toTopOf="@+id/switch3"
      app:layout_constraintTop_toBottomOf="@+id/imageView2"
      app:layout_constraintVertical_bias="0.205"
      tools:layout_editor_absoluteX="235dp" />

  <Switch
      android:id="@+id/switch3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Switch"
      tools:layout_editor_absoluteX="235dp"
      tools:layout_editor_absoluteY="330dp" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="128dp"
      android:text="Personalized Offers"
      app:layout_constraintBottom_toTopOf="@+id/textView10"
      app:layout_constraintTop_toBottomOf="@+id/imageView2"
      app:layout_constraintVertical_bias="0.145"
      tools:layout_editor_absoluteX="64dp" />

  <TextView
      android:id="@+id/textView10"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Connect to Facebook"
      tools:layout_editor_absoluteX="16dp"
      tools:layout_editor_absoluteY="338dp" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Language"
      tools:layout_editor_absoluteX="17dp"
      tools:layout_editor_absoluteY="405dp" />

  <Spinner
      android:id="@+id/spinner"
      android:layout_width="283dp"
      android:layout_height="wrap_content"
      tools:layout_editor_absoluteX="115dp"
      tools:layout_editor_absoluteY="405dp" />

  <TextView
      android:id="@+id/textView13"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Change Nickname"
      tools:layout_editor_absoluteX="17dp"
      tools:layout_editor_absoluteY="462dp" />

  <EditText
      android:id="@+id/editTextTextPersonName3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:inputType="textPersonName"
      android:text="Name"
      tools:layout_editor_absoluteX="175dp"
      tools:layout_editor_absoluteY="462dp" />

  <Button
      android:id="@+id/button"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Help & Support"
      tools:layout_editor_absoluteX="16dp"
      tools:layout_editor_absoluteY="531dp" />

  <Button
      android:id="@+id/button2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Notifications"
      tools:layout_editor_absoluteX="235dp"
      tools:layout_editor_absoluteY="591dp" />

  <Button
      android:id="@+id/button3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Privacy"
      tools:layout_editor_absoluteX="16dp"
      tools:layout_editor_absoluteY="591dp" />

  <Button
      android:id="@+id/button5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Terms of Service"
      tools:layout_editor_absoluteX="207dp"
      tools:layout_editor_absoluteY="531dp" />

  <Button
      android:id="@+id/button6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Rate This App"
      tools:layout_editor_absoluteX="16dp"
      tools:layout_editor_absoluteY="650dp" />

  <Button
      android:id="@+id/button7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Delete Account"
      tools:layout_editor_absoluteX="206dp"
      tools:layout_editor_absoluteY="650dp" />

  <ImageView
      android:id="@+id/imageView2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintBottom_toTopOf="@+id/textView14"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/settings_img" />

  <TextView
      android:id="@+id/textView14"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Settings"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      tools:layout_editor_absoluteY="155dp" />
</androidx.constraintlayout.widget.ConstraintLayout>